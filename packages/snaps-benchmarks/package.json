{
  "name": "snaps-benchmarks",
  "version": "0.33.1-flask.1",
  "scripts": {
    "start": "yarn ts-node --swc src/server.ts",
    "test": "jest && yarn posttest",
    "posttest": "jest-it-up",
    "test:ci": "yarn test",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path ../../.gitignore",
